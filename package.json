{
	"name": "@ctx-core/html",
	"version": "11.6.24",
	"description": "ctx-core html",
	"keywords": [
		"ctx-core",
		"html"
	],
	"homepage": "https://github.com/ctx-core/html#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/html/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/html.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./attr_",
		"./attrs_",
		"./class_",
		"./class__",
		"./dataset__data_attrs",
		"./html_",
		"./js_html_",
		"./links_html_",
		"./style_",
		"./style__",
		"./style__assign",
		"./styles_o_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/function": "21.22.20",
		"@ctx-core/object": "^31.1.6"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.4.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}